# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration
NEXT_PUBLIC_API_URL=your_api_url

# Arweave/STOAR Configuration
# Optional: If not provided, will use browser wallet (ArConnect)
NEXT_PUBLIC_ARWEAVE_WALLET_KEY=your_arweave_wallet_json_key
# Optional: Custom Arweave gateway (defaults to https://arweave.net)
NEXT_PUBLIC_ARWEAVE_GATEWAY=https://arweave.net

# Optional: Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=your_google_analytics_id